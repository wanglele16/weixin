.index-container{display:flex;flex-direction:column;align-items:center;height:100%}.index-head{width:100%;position:relative;padding-bottom:5px;padding-top:5px}.scroll1{width:85%;height:28px;white-space:nowrap}.scroll1 li{display:inline-block;height:20px;line-height:20px;padding:2px 10px;margin:2px 8px 2px 0}.active{background:#f5f5f5 url(/images/active.png) no-repeat right bottom;background-size:15%;border-radius:10px;color:#f00}.plus{position:absolute;width:28px;height:28px;top:6px;right:15px;background:url(/images/plus.png) no-repeat right top 5px}.index-body{flex:1;width:100%}.course{display:-webkit-flex;display:-ms-flex;display:flex;flex-direction:column;box-sizing:border-box;font-size:12px;padding:0 10px 10px;width:100%}.course .courseHead{height:34px;line-height:34px;width:80%;white-space:nowrap;text-overflow:ellipsis;overflow:hidden}
